<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.dao.CarsmarksEntity" table="carsmarks" schema="public" catalog="car_sharing_service">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="mark">
            <column name="mark" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <set name="carsmodelsById" inverse="true">
            <key>
                <column name="idmark"/>
            </key>
            <one-to-many not-found="ignore" class="org.example.dao.CarsmodelsEntity"/>
        </set>
    </class>
</hibernate-mapping>