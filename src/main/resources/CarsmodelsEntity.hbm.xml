<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.dao.CarsmodelsEntity" table="carsmodels" schema="public" catalog="car_sharing_service">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="model">
            <column name="model" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="idmark">
            <column name="idmark" sql-type="integer"/>
        </property>
        <many-to-one name= class="org.example.dao.CarsmarksEntity">
            <column name="idmark"/>
        </many-to-one>
        <set name="carsById" inverse="true">
            <key>
                <column name="idmodel"/>
            </key>
            <one-to-many not-found="ignore" class="org.example.dao.CarsEntity"/>
        </set>
    </class>
</hibernate-mapping>