<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.dao.ExtrausersdataEntity" table="extrausersdata" schema="public"
           catalog="car_sharing_service">
        <id name="idpassport">
            <column name="idpassport" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="lastname">
            <column name="lastname" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="birthdate">
            <column name="birthdate" sql-type="date" not-null="true"/>
        </property>
        <property name="drivinglicense">
            <column name="drivinglicense" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="registerdate">
            <column name="registerdate" sql-type="date" not-null="true"/>
        </property>
        <set name="usersByIdpassport" inverse="true">
            <key>
                <column name="idextrausersdata" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="org.example.dao.UsersEntity"/>
        </set>
    </class>
</hibernate-mapping>